{
  "name": "please",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "dev-ios": "react-native run-ios --simulator='iPhone X'",
    "dev-android": "react-native run-android",
    "release-android": "react-native run-android --variant=release",
    "test": "jest",
    "e2e": "detox test --configuration ios.sim.debug",
    "e2e:build": "detox build",
    "format": "prettier --write '**/*.{js,json}'",
    "lint": "eslint -c .eslintrc --ext .js src"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "eslint -c .eslintrc --ext .js",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "appcenter": "^1.7.1",
    "appcenter-analytics": "^1.7.1",
    "appcenter-crashes": "^1.7.1",
    "await-to-js": "^2.0.1",
    "babel-eslint": "^8.2.6",
    "color": "^3.0.0",
    "eslint": "^5.4.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.1",
    "headersfactory": "^1.0.3",
    "idx": "^2.4.0",
    "lodash": "^4.17.10",
    "moment": "^2.22.2",
    "prop-types": "^15.6.2",
    "react": "16.4.1",
    "react-data-fetching": "^0.2.2",
    "react-native": "0.56.0",
    "react-native-snap-carousel": "^3.7.2",
    "react-native-swipe-up-down": "^1.0.5",
    "react-native-vector-icons": "^5.0.0",
    "react-navigation": "^2.9.3",
    "react-pose": "^2.2.0",
    "react-redux": "^5.0.7",
    "redux": "^4.0.0",
    "redux-thunk": "^2.3.0",
    "styled-components": "^3.3.3",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@babel/core": "7.0.0-beta.47",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "23.4.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "babel-preset-react-native": "^5",
    "detox": "^8.1.1",
    "fetch-mock": "^6.5.2",
    "husky": "^0.14.3",
    "jest": "23.4.2",
    "lint-staged": "^7.2.0",
    "mocha": "^5.2.0",
    "prettier": "^1.14.0",
    "react-test-renderer": "^16.4.2",
    "redux-mock-store": "^1.5.3"
  },
  "jest": {
    "preset": "react-native",
    "moduleNameMapper": {
      "styled-components": "<rootDir>/node_modules/styled-components/dist/styled-components.native.cjs.js"
    },
    "setupFiles": [
      "<rootDir>/node_modules/appcenter-analytics/test/AppCenterAnalyticsMock.js",
      "<rootDir>/node_modules/appcenter-crashes/test/AppCenterCrashesMock.js",
      "<rootDir>/node_modules/appcenter/test/AppCenterMock.js"
    ]
  },
  "detox": {
    "configurations": {
      "ios.sim.debug": {
        "binaryPath": "ios/build/Build/Products/Debug-iphonesimulator/please.app",
        "build": "xcodebuild -project ios/please.xcodeproj -scheme please -configuration Debug -sdk iphonesimulator -derivedDataPath ios/build",
        "type": "ios.simulator",
        "name": "iPhone X"
      }
    },
    "test-runner": "jest"
  }
}
